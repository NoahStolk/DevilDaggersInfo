@page "/custom/entry/{Id:int}"
@using DevilDaggersInfo.Web.BlazorWasm.Client.Core.CanvasChart.Components

@if (GetCustomEntryData == null)
{
	<Loading />
}
else
{
	<Heading Title="@($"{GetCustomEntryData.PlayerName}'s score on {GetCustomEntryData.SpawnsetName}")" />

	@foreach (var lc in _lineCharts)
	{
		<Heading Title="@lc.Name" Level="2" />
		<LineChart DataOptions="lc.DataOptions" UniqueName="@($"#{lc.Name.ToLower()}-chart")" DataSets="@lc.Sets" Options="lc.ChartOptions" />
	}
}
