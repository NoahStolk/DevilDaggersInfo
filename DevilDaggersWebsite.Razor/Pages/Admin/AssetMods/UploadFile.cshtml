@page
@model UploadFileModel
@{
	ViewData["Title"] = "Upload AssetMod File";
}

<h1>@ViewData["Title"]</h1>

<p>Notes</p>
<ul>
	<li>File must not be larger than 256 * 1024 * 1024 bytes in length.</li>
	<li>File name must not be over 80 characters in length.</li>
	<li>File must be a .zip file.</li>
	<li>File name must be unique.</li>
	<li>All files within the zip archive must be valid Devil Daggers binaries.</li>
	<li>All Devil Daggers binaries within the zip archive must contain assets.</li>
	<li>Please don't use any special Unicode characters inside zip archive entry names. This is a limitation in .NET. (dd-ÆTHEREA will show up as dd-�THEREA.)</li>
</ul>

<form method="post" enctype="multipart/form-data">
	<input asp-for="FormFile" />
	<input type="submit" />
</form>

@section Styles {
	<link rel='stylesheet' href='~/styles/admin.css' asp-append-version="true" />
}
